algoritmo
	DECLARE
	LITERAL op , a;
	NUMERICO s,d;
	Nulo a;
	s <--  5+5;
	op <-- "ads";
		escreva("s + a");
	leia(op);
	se(op=falso) inicio
		escreva("Oi, eu sou o Goku!");
	fim

	senao inicio
		escreva("Seu inseto, achei que iria preferir o Kakaroto");
	fim
fim algoritmo

subrotina fazNada(v1 nulo)
	escreva("Se a rotina faz nada, por que tem um comando de impressao?");
fim subrotina
